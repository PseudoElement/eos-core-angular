{
  "name": "eos",
  "version": "0.0.2",
  "license": "MIT",
  "scripts": {
    "ng": "ng",
    "update-build": "node ./build-version.js",
    "start": "ng serve --host 0.0.0.0 --disable-host-check",
    "build": "ng build",
    "dev": "ng build --watch",
    "postinstall": "node ./postinstall.js && node ./build-version",
    "test": "ng test",
    "saas-lint": "./node_modules/sass-lint/bin/sass-lint.js -c ./.sass-lint.yml -v -q",
    "lint": "ng lint --type-check",
    "e2e": "ng e2e",
    "build-stage": "node ./build-version.js && ng build -dev -pr=false -ec",
    "build-prod": "node ./build-version.js && ng build --prod && npm run copy-plugin-scripts",
    "dev-server": "web-dev-server --port 37010 --root-dir ./dist --base-path /classif",
    "copy-plugin-scripts": "node ./scripts/copy.js"
  },
  "pre-commit": [
    "lint"
  ],
  "private": true,
  "dependencies": {
    "@angular/animations": "9.1.13",
    "@angular/common": "9.1.13",
    "@angular/compiler": "9.1.13",
    "@angular/core": "9.1.13",
    "@angular/forms": "9.1.13",
    "@angular/http": "7.2.14",
    "@angular/platform-browser": "9.1.13",
    "@angular/platform-browser-dynamic": "9.1.13",
    "@angular/router": "9.1.13",
    "@eos/jsplugins-manager": "2.0.2",
    "angular2-uuid": "^1.1.1",
    "core-js": "^2.6.5",
    "file-saver": "^2.0.2",
    "fs-extra": "^4.0.3",
    "moment": "^2.24.0",
    "ng2-dragula": "^1.5.0",
    "ngx-bootstrap": "5.5.0",
    "rxjs": "^6.5.5",
    "stream": "0.0.2",
    "timers": "^0.1.1",
    "tslib": "^1.10.0",
    "web-animations-js": "^2.3.2",
    "zone.js": "~0.10.2"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.901.15",
    "@angular/cli": "^9.1.15",
    "@angular/compiler-cli": "9.1.13",
    "@angular/language-service": "9.1.13",
    "@types/jasmine": "2.5.45",
    "@types/node": "^12.11.1",
    "@types/xml2js": "0.4.5",
    "bootstrap-sass": "^3.4.1",
    "codelyzer": "^5.1.2",
    "font-awesome": "^4.7.0",
    "jasmine-core": "~2.6.2",
    "jasmine-spec-reporter": "~4.1.0",
    "karma": "~1.7.0",
    "karma-chrome-launcher": "~2.1.1",
    "karma-cli": "~1.0.1",
    "karma-coverage-istanbul-reporter": "^1.4.3",
    "karma-jasmine": "^1.1.2",
    "karma-jasmine-html-reporter": "^0.2.2",
    "pre-commit": "^1.2.2",
    "protractor": "^5.4.4",
    "ts-node": "~3.0.4",
    "tslint": "~5.16.0",
    "typescript": "3.8.3"
  },
  "engines": {
    "node": ">=14.17.0"
  }
}