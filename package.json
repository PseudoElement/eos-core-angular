{
  "name": "eos",
  "version": "0.0.2",
  "license": "MIT",
  "scripts": {
    "ng": "ng",
    "update-build": "node ./build-version.js",
    "start": "ng serve --host 0.0.0.0 --disable-host-check",
    "test": "ng test",
    "saas-lint": "./node_modules/sass-lint/bin/sass-lint.js -c ./.sass-lint.yml -v -q",
    "lint": "ng lint --type-check",
    "e2e": "ng e2e",
    "build-stage": "node ./build-version.js && ng build -dev -pr=false -ec",
    "build-prod": "node ./build-version.js && ng build --configuration production && npm run copy-plugin-scripts",
    "dev-server": "web-dev-server --port 37010 --root-dir ./dist/box --base-path /classif",
    "copy-plugin-scripts": "node ./scripts/copy.js",
    "build-lib-watch": "ng build delo-lib --watch",
    "build-lib": "npm run prebuild && ng build delo-lib",
    "build-resources": "npm run bandle-scss && npm run copy-assets",
    "watch": "ng build --watch --configuration development",
    "build": "ng build && npm run copy-plugin-scripts",
    "bandle-scss": "scss-bundle scss-bundle -e ./projects/delo-lib/src/lib/styles/header.scss -d ./projects/delo-lib/src/lib/styles/  -o ./dist/delo-lib/lib/styles/header.scss && scss-bundle scss-bundle -e ./projects/delo-lib/src/lib/styles/footer.scss -d ./projects/delo-lib/src/lib/styles/  -o ./dist/delo-lib/lib/styles/footer.scss",
    "copy-assets": "cpx \"./projects/delo-lib/src/lib/assets/**/*\" \"./dist/delo-lib/lib/assets\" --verbose",
    "prebuild": "node \"./scripts/replEnv.js\""
  },
  "pre-commit": [],
  "private": true,
  "dependencies": {
    "@angular/animations": "15.1.5",
    "@angular/common": "15.1.5",
    "@angular/compiler": "15.1.5",
    "@angular/core": "15.1.5",
    "@angular/forms": "15.1.5",
    "@angular/http": "7.2.14",
    "@angular/platform-browser": "15.1.5",
    "@angular/platform-browser-dynamic": "15.1.5",
    "@angular/router": "15.1.5",
    "@eos/jsplugins-manager": "2.0.2",
    "angular2-uuid": "^1.1.1",
    "core-js": "^2.6.5",
    "file-saver": "^2.0.2",
    "fs-extra": "^4.0.3",
    "moment": "^2.24.0",
    "ng2-dragula": "^1.5.0",
    "ngx-bootstrap": "^5.5.0",
    "rxjs": "^6.5.5",
    "stream": "0.0.2",
    "timers": "^0.1.1",
    "tslib": "^2.0.0",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "15.1.6",
    "@angular/cli": "15.1.6",
    "@angular/compiler-cli": "15.1.5",
    "@angular/language-service": "15.1.5",
    "@types/jasmine": "~3.6.0",
    "@types/node": "^12.11.1",
    "@types/xml2js": "0.4.5",
    "ajv": "^8.12.0",
    "bootstrap-sass": "^3.4.1",
    "cpx": "^1.5.0",
    "codelyzer": "^6.0.0",
    "font-awesome": "^4.7.0",
    "jasmine-core": "~3.6.0",
    "jasmine-spec-reporter": "~5.0.0",
    "karma": "~6.4.1",
    "karma-chrome-launcher": "~3.1.0",
    "karma-cli": "~1.0.1",
    "karma-coverage-istanbul-reporter": "~3.0.2",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "^1.5.0",
    "ng-packagr": "^15.1.0",
    "pre-commit": "^1.2.2",
    "protractor": "~7.0.0",
    "ts-node": "~3.0.4",
    "tslint": "~6.1.0",
    "scss-bundle": "^3.1.2",
    "typescript": "4.9.5"
  },
  "engines": {
    "node": ">=14.17.0"
  }
}