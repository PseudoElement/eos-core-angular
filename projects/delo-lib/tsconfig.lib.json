/* To learn more about this file see: https://angular.io/config/tsconfig. */
{
  "extends": "../../tsconfig.json",
  "compilerOptions": {
    "outDir": "../../out-tsc/lib",
    "target": "es2015",
    "declaration": true,
    "declarationMap": true,
    "inlineSources": true,
    "noImplicitAny": false,
    "strictPropertyInitialization": false,
    "noStrictGenericChecks": false,
    "strictNullChecks": false,
    "noImplicitReturns": false,
    "types": [],
    "lib": [
      "dom",
      "es2018"
    ]
  },
  "exclude": [
    "**/*.spec.ts"
  ]
}
